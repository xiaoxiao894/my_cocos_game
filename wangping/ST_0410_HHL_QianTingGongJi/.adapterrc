{
  "injectOptions": {
    "Unity": {
      "body": "<script>const AdvChannels='{{__adv_channels_adapter__}}';const storeIOS='https://apps.apple.com/us/app/stormshot2-boom-blast/id6532591623?ct=Tap33752560';const storeANDROID='https://play.google.com/store/apps/details?id=com.funplus.st.region';class Platform{constructor(){this._lockStore=false}static get instance(){if(!this._instance)this._instance=new Platform();return this._instance}init(){this._lockStore=false}jumpStore(){console.log(`平台：${AdvChannels} lockstore:${this._lockStore}`);switch(AdvChannels){case'AppLovin':case'Unity':if(/android/i.test(navigator.userAgent)){if(this._lockStore)return;this._lockStore=true;window.mraid.open(storeANDROID);setTimeout(()=>{this._lockStore=false},500)}else{if(this._lockStore)return;this._lockStore=true;window.mraid.open(storeIOS);setTimeout(()=>{this._lockStore=false},500)}if(!window.mraid){this.defaultJump();console.error(`${AdvChannels}平台包 当前环境没有 mraid 环境`)}break;case'IronSource':if(this._lockStore)return;this._lockStore=true;dapi?.openStoreUrl();setTimeout(()=>{this._lockStore=false},500);if(!dapi){this.defaultJump();console.error(`${AdvChannels}平台包 当前环境没有 dapi 环境`)}break;case'Moloco':if(typeof window.FbPlayableAd!=='undefined'){window.FbPlayableAd.onCTAClick()}else{this.defaultJump();console.error(`${AdvChannels}平台包 当前环境没有 FbPlayableAd 环境`)}break;case'Google':if(this._lockStore)return;this._lockStore=true;ExitApi?.exit();setTimeout(()=>{this._lockStore=false},500);if(!ExitApi){this.defaultJump();console.error(`${AdvChannels}平台包 当前环境没有 ExitApi 环境`)}break;default:this.defaultJump();console.error(`${AdvChannels}平台未接入或接入有问题，请联系开发人员`);break}}defaultJump(){window.open(/android/i.test(navigator.userAgent)?storeANDROID:storeIOS,'_blank')}}Platform._instance=null;</script>"
    }
  }
}
